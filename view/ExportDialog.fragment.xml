<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog class="sapUiSmallMarginBottom" id="idExportDialog" title="{i18n>EXPRT_PDF}">
		<List id="idExprtLst">
			<items>
				<InputListItem label="{i18n>LBL_ALL}">
					<CheckBox id="idCheckBox" select="onSelectAll"/>
				</InputListItem>
				<InputListItem label="{i18n>ACADEMICS}">
					<CheckBox id="idAcadItm" select="onHandleSelect"/>
				</InputListItem>
				<InputListItem label="{i18n>ADVISOR_NOTES}">
					<CheckBox id="idAdvNtsItm" select="onHandleSelect"/>
				</InputListItem>
				<InputListItem label="{i18n>HOLDS}">
					<CheckBox id="idHoldsItm" select="onHandleSelect"/>
				</InputListItem>
				<InputListItem label="{i18n>ACTIVITIES}">
					<CheckBox id="idActvtsItm" select="onHandleSelect"/>
				</InputListItem>
			</items>
		</List>
		<beginButton>
			<Button press="handleExprtOK" text="{i18n>OK}"></Button>
		</beginButton>
		<endButton>
			<Button press="handleExprtCancel" text="{i18n>CANCEL}"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>